<% provide(:title, "ダッシュボード") %>

<%= content_tag(:div, @user.photos.count) %>
<% @user.photos.each do |photo| %>
  <%= content_tag(:div, photo.took_date) %>
  <%= link_to image_tag("layouts/sugoi.jpg", :size => "200x200", :id => "", :alt => "") , "#" %>
  <% photo.comments.each do |comment| %>
    <%= content_tag(:div, comment.content) %>
  <% end %>
<% end %>
